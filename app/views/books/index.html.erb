<h1>Books</h1>
<% @books.each do |book| %>
<table>
  <thead></thead>

<span><%= book.title %></span>

<span><%= book.body %></span>
</table>

</div>
<% end %>

<h2>New book</h2>
<%= form_with model: @book do |f| %>
  <p>Title</p>
  <%= f.text_field :title %>

  <p>body</p>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<% end %>